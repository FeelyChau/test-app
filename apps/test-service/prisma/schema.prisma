datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  heads     Head[]
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Head {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  test      String
  updatedAt DateTime @updatedAt
  users     User?    @relation(fields: [usersId], references: [id])
  usersId   String?
}

model Order {
  createdAt DateTime @default(now())
  headeraaa String?
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
